#include <stdio.h>

// Function prototype
int add(int a, int b);

int main() {
    int result = add(5, 3);
    printf("Result: %d\n", result);

    return 0;
}

// Function definition
int add(int a, int b) {
    return a + b;
}

<?php
// Function prototype (declaration)
function add($a, $b);

// Function call
$result = add(5, 3);
echo "Result: $result\n";

// Function definition
function add($a, $b) {
    return $a + $b;
}
?>
