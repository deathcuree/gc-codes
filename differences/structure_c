#include <stdio.h>

// Struct definition
struct Person {
    char name[50];
    int age;
};

int main() {
    // Struct initialization
    struct Person person1 = {"Alice", 25};

    // Accessing struct members
    printf("Name: %s\n", person1.name);
    printf("Age: %d\n", person1.age);

    return 0;
}
